//Program to check whether a given number is  a prime or not
#include <stdio.h>
int main(){
    int n, i, flag=0;
    printf("Enter an integer\n");
    scanf("%d", &n);
    for(i=2;i<n;i++){
        if(n%i==0)
            flag=1;
    }
    if(flag)
        printf("%d is not a prime.\n", n);
    else
        printf("%d is a prime.\n", n);
    return 0;
}
